import java.util.Scanner;

public class Main {
public static void main(String[] args) {
Scanner scanner = new Scanner(System.in);

System.out.print("Введите первое число: ");
int number1 = scanner.nextInt();

System.out.print("Введите второе число: ");
int number2 = scanner.nextInt();

System.out.print("Введите третье число: ");
int number3 = scanner.nextInt();

// Сравниваем числа и меняем их местами при необходимости
if (number1 > number2) {
int temp = number1;
number1 = number2;
number2 = temp;
}
if (number2 > number3) {
int temp = number2;
number2 = number3;
number3 = temp;
}
if (number1 > number2) {
int temp = number1;
number1 = number2;
number2 = temp;
}

// Выводим числа в порядке возрастания
System.out.println("Числа в порядке возрастания: " + number1 + ", " + number2 + ", " + number3);
}
}
